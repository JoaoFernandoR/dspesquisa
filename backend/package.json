  "description": "Back-end da aplicação dspesquisa",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx ts-node-dev --transpile-only --ignore-watch node_modules --respawn index.ts",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
